    </item>
    <item>
        <shape>
            <corners android:radius="3dp"/>
            <solid android:color="#ffb3b3b3"/>
            <padding android:bottom="1dp" android:top="1dp" android:left="1dp" android:right="1dp"/>
        </shape>
    </item>
</selector>